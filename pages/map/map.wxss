.container {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.search-bar {
  display: flex;
  padding: 10px;
  background-color: #f5f5f5;
}
.search-bar input {
  flex: 1;
  padding: 8px;
  border: 1px solid #ddd;
  border-radius: 4px;
}
.search-bar button {
  margin-left: 10px;
  padding: 8px 12px;
  background-color: #4CAF50;
  color: white;
  border: none;
  border-radius: 4px;
}